<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="tableView" prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EvenKeyController">
    <columns>
        <TableColumn sortable="false" fx:id="evenIndex" prefWidth="75.0" text="Парний індекс" />
        <TableColumn sortable="false" fx:id="k" prefWidth="75.0" text="K Ключ шифрування" />
        <TableColumn sortable="false" fx:id="kq" prefWidth="75.0" text="K(q) Допоміжний ключ" />
        <TableColumn sortable="false" fx:id="si" prefWidth="75.0" text="Значення зсуву S(i)" />
        <TableColumn sortable="false" fx:id="oper1" prefWidth="75.0" text="K = K &lt;&lt; S(i)" />
        <TableColumn sortable="false" fx:id="oper2" prefWidth="75.0" text="C" />
        <TableColumn sortable="false" fx:id="oper3" prefWidth="75.0" text="Y = C &lt;&lt; i/2" />
        <TableColumn sortable="false" fx:id="oper4" prefWidth="75.0" text="Y = Y ⊞ K(q)" />
        <TableColumn sortable="false" fx:id="oper5" prefWidth="75.0" text="G = K ⊞ Y" />
        <TableColumn sortable="false" fx:id="oper6" prefWidth="75.0" text="G = Box(G)" />
        <TableColumn sortable="false" fx:id="oper7" prefWidth="75.0" text="G = Rows(G)" />
        <TableColumn sortable="false" fx:id="oper8" prefWidth="75.0" text="G = Y ⨁ G" />
        <TableColumn sortable="false" fx:id="oper9" prefWidth="75.0" text="G = Box(G)" />
        <TableColumn sortable="false" fx:id="oper10" prefWidth="75.0" text="G = Rows(G)" />
        <TableColumn sortable="false" fx:id="oper11" prefWidth="75.0" text="G = Col(G)" />
        <TableColumn sortable="false" fx:id="oper12" prefWidth="75.0" text="G = K ⊞ G" />
        <TableColumn sortable="false" fx:id="result" prefWidth="75.0" text="Результат" />
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
</TableView>
