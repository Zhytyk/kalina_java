<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="tableView" prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.OddKeyController">
    <columns>
        <TableColumn sortable="false" fx:id="oddIndex" prefWidth="75.0" text="Непарний індекс (i)" />
        <TableColumn sortable="false" fx:id="evenKey" prefWidth="75.0" text="ключ з парним індексом K(i-1) " />
        <TableColumn sortable="false" fx:id="si" prefWidth="75.0" text="Значення зсуву si" />
        <TableColumn sortable="false" fx:id="result" prefWidth="75.0" text="Результат si &lt;&lt;&lt; K(i-1)" />
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
    </columnResizePolicy>
</TableView>
